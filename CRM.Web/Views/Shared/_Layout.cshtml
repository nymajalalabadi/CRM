<!doctype html>
<html class="no-js " lang="en">

<head>
    <partial name="_MetaTags"></partial>
    <title>نرم افزار CRM - @ViewData["Title"]</title>
    <partial name="_HeaderRefrences" />
    @RenderSection("Styles", false)
</head>
<body class="theme-cyan">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30"><img class="zmdi-hc-spin" src="/images/logo.svg" width="48" height="48" alt="قطب نما"></div>
            <p>لطفا صبر کنید ...</p>
        </div>
    </div>
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>

    <!-- Top Bar -->
    <partial name="_TopBar" />

    <!-- Right Sidebar -->
    <partial name="_RightSideBar" />

    <!-- Left Sidebar -->
    <partial name="_LeftSideBar" />

    <!-- Chat-launcher -->
    <partial name="_ChatLauncher" />

    <!-- Main Content -->

    <section class="content home">

        @RenderSection("Block_Header", false)

        <div class="container-fluid">

            @RenderBody()

        </div>

    </section>

    <!-- Jquery Core Js -->
    <partial name="_FooterRefrences" />

    <partial name="_SiteMessages" />

    @RenderSection("Scripts", false)
</body>

</html>